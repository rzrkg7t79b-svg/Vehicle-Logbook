{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "baseUrl": "..",
    "paths": {}
  },
  "include": ["./**/*.ts", "../shared/**/*.ts"],
  "exclude": ["node_modules"]
}
